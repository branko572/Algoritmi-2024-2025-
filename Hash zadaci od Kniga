–ü–æ–¥–∞—Ç–æ—á–Ω–∏ —Å—Ç—Ä—É–∫—Ç—É—Ä–∏
–µ–ª–µ–º–µ–Ω—Ç —Å–æ –∫–ª—É—á –û–ø—à—Ç–∏–Ω–∞ –≤–æ —Å–æ–æ–¥–≤–µ—Ç–Ω–∞—Ç–∞ —Ö–µ—à —Ç–∞–±–µ–ª–∞. –î–æ–∫–æ–ª–∫—É –Ω–µ –ø–æ—Å—Ç–æ–∏, –≤–æ –∏–∑–±—Ä–∞–Ω–∞—Ç–∞ —Ö–µ—à —Ç–∞–±–µ–ª–∞ —Å–µ –¥–æ–¥–∞–≤–∞ –µ–ª–µ–º–µ–Ω—Ç –≤–æ —Ñ–æ—Ä–º–∞—Ç (–û–ø—à—Ç–∏–Ω–∞, 1). –î–æ–∫–æ–ª–∫—É –ø–æ—Å—Ç–æ–∏
–µ–ª–µ–º–µ–Ω—Ç —Å–æ –∫–ª—É—á –û–ø—à—Ç–∏–Ω–∞, –≤–æ —Ç–æj —Å–ª—É—á–∞j —Å–µ –∑–µ–º–∞ –º–æ–º–µ–Ω—Ç–∞–ª–Ω–∞—Ç–∞ –≤—Ä–µ–¥–Ω–æ—Å—Ç –Ω–∞ —Ç–æj
–µ–ª–µ–º–µ–Ω—Ç –∏ –Ω–∞ –Ω–µ–∞ —Å–µ –¥–æ–¥–∞–≤–∞ 1. –¢–∞–∫–∞ –ø—Ä–æ–º–µ–Ω–µ—Ç–∏–æ—Ç –µ–ª–µ–º–µ–Ω—Ç —Å–µ –¥–æ–¥–∞–≤–∞ –≤–æ —Å–æ–æ–¥–≤–µ—Ç–Ω–∞—Ç–∞
—Ö–µ—à —Ç–∞–±–µ–ª–∞.
–ü—Ä–µ–±–∞—Ä—É–≤–∞—ö–µ—Ç–æ —Å–ø–æ—Ä–µ–¥ –∫–ª—É—á –∏ –æ–±—Ä–∞–±–æ—Ç–∫–∞—Ç–∞ –Ω–∞ —Ä–µ–∑—É–ª—Ç–∞—Ç–∏—Ç–µ —Å–µ —Ä–µ–∞–ª–∏–∑–∏—Ä–∞ –Ω–∞
—Å–ª–µ–¥–Ω–∏–æ—Ç –Ω–∞—á–∏–Ω: –ø—Ä–≤–æ —Å–µ —á–∏—Ç–∞ –∫–ª—É—á–æ—Ç –ø–æ –∫–æj ¬¥–∫–µ —Å–µ –ø—Ä–µ–±–∞—Ä—É–≤–∞ –∏ —Å–µ –¥–µ—Ñ–∏–Ω–∏—Ä–∞ –ø—Ä–æ–º–µ–Ω–ª–∏–≤–∞ positiveCount –≤–æ –∫–æj–∞ —Å–µ —Å–º–µ—Å—É–≤–∞ –≤—Ä–µ–¥–Ω–æ—Å—Ç–∞ –Ω–∞ –ø–æ–∑–∏—Ç–∏–≤–Ω–∏—Ç–µ –ø–∞—Ü–∏–µ–Ω—Ç–∏ –∑–∞
–¥–∞–¥–µ–Ω–∏–æ—Ç –∫–ª—É—á. –ò—Å—Ç–∞—Ç–∞ –ø–æ—Å—Ç–∞–ø–∫–∞ —Å–µ –ø–æ–≤—Ç–æ—Ä—É–≤–∞ –∏ –∑–∞ —Ö–µ—à —Ç–∞–±–µ–ª–∞—Ç–∞ —Å–æ –Ω–µ–≥–∞—Ç–∏–≤–Ω–∏
–ø–∞—Ü–∏–µ–Ω—Ç–∏ —Å–æ –ø—Ä–æ–º–µ–Ω–ª–∏–≤–∞—Ç–∞ negativeCount. –ù–∞ –∫—Ä–∞j–æ—Ç —Å–µ –ø–µ—á–∞—Ç–∏ —Ä–∏–∑–∏–∫ —Ñ–∞–∫—Ç–æ—Ä–æ—Ç
–∑–∞ –¥–∞–¥–µ–Ω–∞—Ç–∞ –æ–ø—à—Ç–∏–Ω–∞.
–ò–º–ø–ª–µ–º–µ–Ω—Ç–∞—Ü–∏j–∞—Ç–∞ –Ω–∞ —Ä–µ—à–µ–Ω–∏–µ—Ç–æ –µ –¥–∞–¥–µ–Ω–∞ –ø–æ–¥–æ–ª—É


public class Main {
    public static void main(String[] args){
     Scanner sc = new Scanner(System.in);
     int n = sc.nextInt();
        sc.nextLine(); // consume the leftover newline

        int pos=0;
     int neg=0;
     CBHT<String, int[]>cbht=new CBHT<>(n*2);
     for (int i = 0; i < n; i++) {
         String s = sc.nextLine();
         String []parts = s.split(" ");
         String opstina = parts[0];
         String prezime = parts[1];
         String status = parts[2];
SLLNode<MapEntry<String, int[]>>node=cbht.search(opstina);
         if(node==null){
             int[]counts=new int[2];
             if(status.equals("positive")){
                 counts[0]++;
             }else if(status.equals("negative")){
                 counts[1]++;
             }
             cbht.insert(opstina, counts);
         }else {
             int[] counts=node.element.value;
             if(status.equals("positive")){
                 counts[0]++;
             }else if(status.equals("negative")){
                 counts[1]++;
             }

         }
     }
        String targetOpstina = sc.nextLine();

        SLLNode<MapEntry<String,int[]>>result=cbht.search(targetOpstina);
     if(result!=null){
         pos=result.element.value[0];
         neg=result.element.value[1];
         double risk=(double) pos/(pos+neg);
         System.out.printf("%.2f\n",risk);
     }else {
         System.out.printf("0.00");
     }
    }
}
